
$(document).ready(function() {
	var variable = "<%=@var%>";
	// alert(variable);

	if(variable == 'basic'){
		$('.outer').empty();
	 $('.outer').append('<%= j render 'user_basic_create_form'%>');
	} else if(variable == 'project'){
		$('.outer').empty();
	 $('.outer').append('<%= j render 'project_update_form'%>');
	}else if(variable == 'education'){
		$('.outer').empty();
	 $('.outer').append('<%= j render 'education_update_form'%>');
	} 


	
	

	
});